<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.uptang.cloud.score.repository.StudentRepository">

    <resultMap id="ResultMap" type="com.uptang.cloud.score.common.model.Student">
        <id property="id" column="id"/>
        <result property="school" column="school"/>
        <result property="schoolId" column="school_id"/>
        <result property="gradeId" column="grade_id"/>
        <result property="gradeName" column="grade_name"/>
        <result property="classId" column="class_id"/>
        <result property="className" column="class_name"/>
        <result property="semesterCode" column="semester_code"/>
        <result property="semesterName" column="semester_name"/>
        <result property="name" column="name"/>
        <result property="gender" column="gender"/>
        <result property="createdTime" column="created_time"/>
        <result property="updatedTime" column="updated_time"/>
    </resultMap>

    <!--字段名 Java属性名-->
    <sql id="columns">
        <trim suffixOverrides=",">
            `id` AS `id`,
            `school` AS `school`,
            `school_id` AS `schoolId`,
            `grade_id` AS `gradeId`,
            `grade_name` AS `gradeName`,
            `class_id` AS `classId`,
            `class_name` AS `className`,
            `semester_code` AS `semesterCode`,
            `semester_name` AS `semesterName`,
            `name` AS `name`,
            `gender` AS `gender`,
            `created_time` AS `createdTime`,
            `updated_time` AS `updatedTime`,
        </trim>
    </sql>
</mapper>